    "vue": "latest"
  },
  "devDependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "google-protobuf": "^3.21.4",
    "prettier": "^3.3.3"
  }
}
