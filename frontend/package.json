{
  "name": "chroma",
  "version": "0.1.8",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8008",
    "build": "vue-cli-service build",
    "protos": "find ../server/proto/protos -type f -name \\*.proto -exec protoc -I ../server/proto/protos --ts_opt=json_names --ts_opt=esModuleInterop=true --ts_opt=no_namespace --ts_out=./src/generated {} \\;"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.15.6",
    "core-js": "^3.8.3",
    "google-protobuf": "^3.21.2",
    "protoc-gen-ts": "^0.8.6",
    "sass": "^1.66.1",
    "scss": "^0.2.4",
    "vue": "^3.3.7",
    "vue-router": "^4.0.13",
    "vuetify": "^3.4.0-alpha.1",
    "webpack-plugin-vuetify": "^2.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^8.52.0",
    "eslint-plugin-vuetify": "^2.0.5",
    "sass-loader": "^13.3.2",
    "typescript": "4.9",
    "vue-template-compiler": "^2.6.14"
  }
}
